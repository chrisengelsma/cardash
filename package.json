{
  "name": "cardash",
  "description": "A car dashboard",
  "author": "Chris Engelsma <chris@hypotemoose.com>",
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/hypotemoose/cardash.git"
  },
  "version": "0.1.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:prod": "ng serve --configuration=production",
    "build": "ng build",
    "build:prod": "ng build --prod",
    "build:element": "npm run build:element:build && npm run build:element:package",
    "build:element:build": "ng build cardash --prod --output-hashing=none",
    "build:element:package": "cat ./dist/cardash/{polyfills,runtime,main}.js > ./dist/cardash.js",
    "build:pages": "ng build --base-href=/cardash/",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "10.0.2",
    "@angular/cdk": "10.0.1",
    "@angular/common": "10.0.2",
    "@angular/compiler": "10.0.2",
    "@angular/core": "10.0.2",
    "@angular/flex-layout": "^10.0.0-beta.32",
    "@angular/forms": "10.0.2",
    "@angular/material": "^10.0.1",
    "@angular/platform-browser": "10.0.2",
    "@angular/platform-browser-dynamic": "10.0.2",
    "@angular/router": "10.0.2",
    "@fortawesome/angular-fontawesome": "^0.6.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.29",
    "@fortawesome/free-solid-svg-icons": "^5.13.1",
    "font-awesome": "^4.7.0",
    "moment-timezone": "^0.5.31",
    "moment": "^2.27.0",
    "rxjs": "^6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "^0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1000.1",
    "@angular/cli": "10.0.1",
    "@angular/compiler-cli": "10.0.2",
    "@angular/elements": "^10.0.2",
    "@angular/language-service": "10.0.2",
    "@types/jasmine": "^3.5.11",
    "@types/jasminewd2": "2.0.8",
    "@types/node": "^14.0.14",
    "@webcomponents/webcomponentsjs": "^2.4.3",
    "codelyzer": "^6.0.0",
    "jasmine-core": "3.5.0",
    "jasmine-spec-reporter": "5.0.2",
    "karma": "5.1.0",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-jasmine": "3.3.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "protractor": "7.0.0",
    "ts-node": "8.10.2",
    "tslint": "6.1.2",
    "typescript": "3.9.6"
  }
}
